table(cellpadding="0", cellspacing="0")
  tr
    th Artist Name

    if permissions.is_sysadmin
      th
  each artist in artists
    tr.display(artist_id="#{artist.id}")
      td #{artist.name}
  
      if permissions.is_sysadmin
        if artist.song_count > 0
          td #{artist.song_count}
        else
          td.delete &#x2327;
