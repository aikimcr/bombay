table(cellpadding="0", cellspacing="0")
  tr
    th Artist Name
    th
  each artist in artists
    tr.display(artist_id="#{artist.id}")
      td #{artist.name}
  
      if system_admin
        if artist.song_count > 0
          td #{artist.song_count}
        else
          if system_admin
            td.delete
              input(type="button", value="-", name="delete")
